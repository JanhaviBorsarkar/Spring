
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id = "address" class = "autowiring.Address" autowire-candidate = "false">
		<property name = "area" value = "Akurdi"></property>
		<property name = "city" value = "Pune"></property>
	</bean>
	
	<bean id = "eaddress" class = "autowiring.Address">
		<property name = "area" value = "Akurdi"></property>
		<property name = "city" value = "Pune"></property>
	</bean>
	
	<bean id = "employee" class = "autowiring.Employee" autowire = "byType">
		<property name = "eid" value = "101"></property>
		<property name = "ename" value = "xyz"></property>
	</bean>

	<bean id = "student" class = "autowiring.Student" autowire = "byType">
		<property name="rno" value = "1"></property>
		<property name = "name" value = "abc"></property>
	</bean>	
	
</beans>			